<h1>Listing day2_awts</h1>

<table>
  <thead>
    <tr>
      <th>Runner1 float time</th>
      <th>Runner1 time</th>
      <th>Runner1</th>
      <th>Runner2 float time</th>
      <th>Runner2 time</th>
      <th>Runner2</th>
      <th>Runner3 float time</th>
      <th>Runner3 time</th>
      <th>Runner3</th>
      <th>Awt float time</th>
      <th>Awt time</th>
      <th>Cat float time</th>
      <th>Cat time</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @day2_awts.each do |day2_awt| %>
      <tr>
        <td><%= day2_awt.runner1_float_time %></td>
        <td><%= day2_awt.runner1_time %></td>
        <td><%= day2_awt.runner1_id %></td>
        <td><%= day2_awt.runner2_float_time %></td>
        <td><%= day2_awt.runner2_time %></td>
        <td><%= day2_awt.runner2_id %></td>
        <td><%= day2_awt.runner3_float_time %></td>
        <td><%= day2_awt.runner3_time %></td>
        <td><%= day2_awt.runner3_id %></td>
        <td><%= day2_awt.awt_float_time %></td>
        <td><%= day2_awt.awt_time %></td>
        <td><%= day2_awt.cat_float_time %></td>
        <td><%= day2_awt.cat_time %></td>
        <td><%= link_to 'Show', day2_awt %></td>
        <td><%= link_to 'Edit', edit_day2_awt_path(day2_awt) %></td>
        <td><%= link_to 'Destroy', day2_awt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

